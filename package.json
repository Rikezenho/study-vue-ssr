{
  "name": "study-vue-ssr",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm:dev-client\" \"npm:dev-server\" \"npm:serve\"",
    "build-and-start": "npm run build-client && npm run build-server && npm run serve",
    "dev-client": "webpack --config webpack/webpack.client.config.js --mode development --watch",
    "dev-server": "webpack --config webpack/webpack.server.config.js --mode development --watch",
    "build-client": "cross-env NODE_ENV=production webpack --config webpack/webpack.client.config.js --mode production",
    "build-server": "cross-env NODE_ENV=production webpack --config webpack/webpack.server.config.js --mode production",
    "serve": "nodemon server/index.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-eslint": "^10.0.2",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "sass": "^1.22.7",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-router": "^3.0.7",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1"
  }
}
